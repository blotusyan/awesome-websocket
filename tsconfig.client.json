{
  "compilerOptions": {
    "target": "ES2017",
    "module": "ES2015",
    "lib": ["DOM", "ES2017"],
    "rootDir": "client",
    "outDir": "public/assets",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["client/**/*.ts", "client/**/*.tsx"]
}
